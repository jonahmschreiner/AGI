	//Cpu Usage
	try {
		Runtime run = Runtime.getRuntime();
		String command = "pidstat | grep \"chrome\"| awk -F \" \" '{print $9}'";
    	Process p = run.exec(new String[] {"/bin/bash","-c",command});
    	BufferedReader stdOutput = new BufferedReader(new InputStreamReader(p.getInputStream()));
		//BufferedReader stdError = new BufferedReader(new InputStreamReader(p.getErrorStream()));
		String cpuUsage = "";
		double totalCpuUsage = 0;
		while (cpuUsage != null){
			cpuUsage = stdOutput.readLine();
			double usage = Double.valueOf(cpuUsage);
			totalCpuUsage += usage;
			System.out.println("Ind: " + usage);
		}
		System.out.println("Total: " + (String)totalCpuUsage);
    } catch (Exception e){
    
    }
 
